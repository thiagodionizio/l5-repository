{
  "name": "thiagodionizio/l5.4-repository",
  "description": "Pacote de Repository Laravel 5.4 adaptado",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Thiago Dionizio",
      "email": "thiago.o.dionizio@gmail.com"
    }
  ],
  "support": {
    "email": "thiago.o.dionizio@gmail.com"
  },
  "require": {
    "illuminate/http": "~5.0",
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/database": "~5.0",
    "illuminate/pagination": "~5.0",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0",
    "prettus/laravel-validation": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.2-dev"
    }
  },
  "minimum-stability": "stable"
}
